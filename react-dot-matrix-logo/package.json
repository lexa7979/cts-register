    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "jest-spec-reporter": "^1.0.8",
    "react": "^16.11.0",
